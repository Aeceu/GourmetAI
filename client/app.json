{
  "expo": {
    "name": "gourmetAI",
    "slug": "gourmetAI",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/food_1.png",
    "scheme": "gourmetAI",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/food_1.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.joseacebuche2.gourmetAI"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4e63506e-243b-4cc2-a885-4019479e5b77"
      }
    }
  }
}
